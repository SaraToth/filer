<main style="border: 1px solid black;">
    <h2><%=locals.currentFolder.folderName%></h2>
    <table>
        <tr>
            <th>Name</th>
            <th>Created</th>
            <th>link</th>
        </tr>
        <% if (locals.files) {%>
            <% files.forEach((file) => {%>
                <tr>
                    <td><%=file.fileName%></td>
                    <td><%=file.createdAt.toLocaleDateString()%></td>
                    <td><%=file.link%></td>
                </tr>
            <%})%>
        <%}%>
    </table>
</main>
